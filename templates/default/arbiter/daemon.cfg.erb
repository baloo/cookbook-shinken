# This file is generated by chef, any local changes will be overwritten next run

define <%= @daemon_type.to_s %> {
  <%= @daemon_type.to_s %>_name    <%= @name %>

  address <%= @address %>
  port    <%= @port %>

<% if @modules %>
  modules <%= @modules.join(" ") %>
<% end %>

<% Hash.new(@variables.to_a.sort).each do |key, value| %>
  <%= key %>  <%= value %>
<% end %>

}

